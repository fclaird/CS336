{

    "version": "0.2.0",
    "configurations": [{
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Python",
            "type": "python",
            "pythonPath": "${config:python.pythonPath}",
            "request": "launch",
            "stopOnEntry": false,
            "console": "none",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ],
            "env": { "name": "value" }
        }


    ],
    "compounds": [



    ]
}